<!doctype html>
<html lang="en">
<%- include('./partials/header'); %>
  
  <body>

<div class="container">
<div class="row">
  <div class="col-4">
    <form method="post" action="/">
      <div class="form-group">
        <label for="species">Species</label>
        <input type="text" class="form-control" name="species" id="species" placeholder="Enter Species" required maxLength="20">
      </div>
      <div class="form-group">
        <label for="lastName">Last Name:</label>
        <input type="text" class="form-control" name="lastName" id="lastName" placeholder="Enter Last Name" required maxLength="20">
      </div>
      <div class="form-group">
        <label for="dateOfBirth">Date of Birth:</label>
        <input type="date" class="form-control" name="dateOfBirth" id="dateOfBirth" required>
      </div>
      <button type="submit" class="btn btn-primary">Submit</button>
    </form>
    </div>
  </div>
</div>




    <%- include('./partials/scripts'); %>


  </body>
</html>