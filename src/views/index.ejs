<!doctype html>
<html lang="en">
<%- include('./partials/header'); %>
  
  <body>

<div class="container">
<div class="row">
  <div class="col-4">
    <form method="post" action="/">
      <div class="form-group">
        <label for="species">Species:</label>
        <input type="text" class="form-control" name="species" id="species" placeholder="Enter Species" required>
      </div>
      <div class="form-group">
        <label for="nickname">Nickname:</label>
        <input type="text" class="form-control" name="nickname" id="nickname" placeholder="Enter Nickname" required>
      </div>
      <div class="form-group">
        <label for="status">Status:</label>
        <input type="status" class="form-control" name="status" id="status" placeholder="Enter Bird Status" required>
      </div>
      <button type="submit" class="btn btn-primary">Submit</button>
    </form>
    </div>
  </div>
</div>




    <%- include('./partials/scripts'); %>


  </body>
</html>